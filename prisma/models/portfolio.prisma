model Profile {
  userId        String  @id
  user          Users   @relation(fields: [userId], references: [id])
  name          String  @default("")
  title         String  @default("")
  location      String  @default("")
  experience    String  @default("")
  description   String  @default("")
  brief         String  @default("")
  aboutImage    String?
  aboutImageAlt String  @default("")
  aboutText     String  @default("")
  githubUrl     String?
  linkedinUrl   String?
  email         String?
  active        Boolean @default(true)
}
